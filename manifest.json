{
    "manifest_version": 3,
    "name": "YouTube Playlist Downloader",
    "version": "1.0",
    "description": "Download YouTube playlists with ease.",
    "permissions": [
      "activeTab",
      "scripting"
    ],
    "action": {
      "default_popup": "popup.html",
      "default_icon": "icon.png"
    }
  }
  